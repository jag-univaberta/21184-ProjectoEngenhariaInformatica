<?xml version="1.0" encoding="utf-8"?>
<LayerDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" version="1.3.0" xsi:noNamespaceSchemaLocation="LayerDefinition-1.3.0.xsd">
  <VectorLayerDefinition>
    <ResourceId>%featuresource%</ResourceId>
    <FeatureName>cadastro:construcoes_view</FeatureName>
    <FeatureNameType>FeatureClass</FeatureNameType>
    <Filter>%filter%</Filter>
    <PropertyMapping>
      <Name>rec_id</Name>
      <Value>rec_id</Value>
    </PropertyMapping>
    <PropertyMapping>
      <Name>tipoconstrucao_id</Name>
      <Value>tipoconstrucao_id</Value>
    </PropertyMapping>
    <PropertyMapping>
      <Name>tipoconstrucao_desc</Name>
      <Value>tipoconstrucao_desc</Value>
    </PropertyMapping>
    <PropertyMapping>
      <Name>talhao_id</Name>
      <Value>talhao_id</Value>
    </PropertyMapping>
    <PropertyMapping>
      <Name>movimentos_sn</Name>
      <Value>movimentos_sn</Value>
    </PropertyMapping>
    <PropertyMapping>
      <Name>codigo</Name>
      <Value>codigo</Value>
    </PropertyMapping>
    <PropertyMapping>
      <Name>designacao</Name>
      <Value>designacao</Value>
    </PropertyMapping>
    <PropertyMapping>
      <Name>label</Name>
      <Value>label</Value>
    </PropertyMapping>
	<Geometry>geometria</Geometry>
    <ToolTip>label</ToolTip>
    <VectorScaleRange>
      <PointTypeStyle>
        <DisplayAsText>false</DisplayAsText>
        <AllowOverpost>false</AllowOverpost>
        <PointRule>
          <LegendLabel />
          <Filter>movimentos_sn='1'</Filter>
          <PointSymbolization2D>
            <Mark>
              <Unit>Points</Unit>
              <SizeContext>DeviceUnits</SizeContext>
              <SizeX>10</SizeX>
              <SizeY>10</SizeY>
              <Rotation>0</Rotation>
              <Shape>Square</Shape>
              <Fill>
                <FillPattern>Solid</FillPattern>
                <ForegroundColor>FF8B008B</ForegroundColor>
                <BackgroundColor>00FFFFFF</BackgroundColor>
              </Fill>
              <Edge>
                <LineStyle>Solid</LineStyle>
                <Thickness>1</Thickness>
                <Color>FF8B008B</Color>
                <Unit>Points</Unit>
				<SizeContext>DeviceUnits</SizeContext>
              </Edge>
            </Mark>
          </PointSymbolization2D>
        </PointRule>
        <PointRule>
          <LegendLabel />
          <Filter>movimentos_sn='0'</Filter>
          <PointSymbolization2D>
            <Mark>
              <Unit>Points</Unit>
              <SizeContext>DeviceUnits</SizeContext>
              <SizeX>10</SizeX>
              <SizeY>10</SizeY>
              <Rotation>0</Rotation>
              <Shape>Square</Shape>
              <Fill>
                <FillPattern>Solid</FillPattern>
                <ForegroundColor>FFDAA520</ForegroundColor>
                <BackgroundColor>00FFFFFF</BackgroundColor>
              </Fill>
              <Edge>
                <LineStyle>Solid</LineStyle>
                <Thickness>1</Thickness>
                <Color>FFDAA520</Color>
                <Unit>Points</Unit>
				<SizeContext>DeviceUnits</SizeContext>
              </Edge>
            </Mark>
          </PointSymbolization2D>
        </PointRule>
      </PointTypeStyle>
      <LineTypeStyle>
        <LineRule>
          <LegendLabel />
          <Filter>movimentos_sn='1'</Filter>
          <LineSymbolization2D>
            <LineStyle>Solid</LineStyle>
            <Thickness>1</Thickness>
            <Color>FF8B008B</Color>
            <Unit>Points</Unit>
			<SizeContext>DeviceUnits</SizeContext>
          </LineSymbolization2D>
        </LineRule>
        <LineRule>
          <LegendLabel />
          <Filter>movimentos_sn='0'</Filter>
          <LineSymbolization2D>
            <LineStyle>Solid</LineStyle>
            <Thickness>1</Thickness>
            <Color>FFDAA520</Color>
            <Unit>Points</Unit>
			<SizeContext>DeviceUnits</SizeContext>
          </LineSymbolization2D>
        </LineRule>
      </LineTypeStyle>
      <AreaTypeStyle>
        <AreaRule>
          <LegendLabel />
          <Filter>movimentos_sn='1'</Filter>
          <AreaSymbolization2D>
            <Fill>
              <FillPattern>Shapefill36</FillPattern>
              <ForegroundColor>FF8B008B</ForegroundColor>
              <BackgroundColor>00FFFFFF</BackgroundColor>
            </Fill>
            <Stroke>
              <LineStyle>Solid</LineStyle>
              <Thickness>1</Thickness>
              <Color>FF8B008B</Color>
              <Unit>Points</Unit>
			  <SizeContext>DeviceUnits</SizeContext>
            </Stroke>
          </AreaSymbolization2D>
        </AreaRule>
        <AreaRule>
          <LegendLabel />
          <Filter>movimentos_sn='0'</Filter>
          <AreaSymbolization2D>
            <Fill>
              <FillPattern>Shapefill15</FillPattern>
              <ForegroundColor>FFDAA520</ForegroundColor>
              <BackgroundColor>00FFFFFF</BackgroundColor>
            </Fill>
            <Stroke>
              <LineStyle>Solid</LineStyle>
              <Thickness>1</Thickness>
              <Color>FFDAA520</Color>
              <Unit>Points</Unit>
			  <SizeContext>DeviceUnits</SizeContext>
            </Stroke>
          </AreaSymbolization2D>
        </AreaRule>
      </AreaTypeStyle>
    </VectorScaleRange>
  </VectorLayerDefinition>
</LayerDefinition>